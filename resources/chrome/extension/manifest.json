{
  "manifest_version": 2,

  "name": "OwNet",
  "description": "This extension inject ownet iframe.",
  "version": "1.0",

  "permissions": [
     "tabs",
     "http://*/*"
  ],

  "content_scripts" : [{
    "matches" : ["http://*/*"],
    "js" : ["inject-iframe.js"]
  }],

  "web_accessible_resources": [
    "inject.js"
  ]
}